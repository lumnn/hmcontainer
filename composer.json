{
    "name": "tsufeki/hmcontainer",
    "description": "Hierarchical dependency injection container inspired by Angular",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "tsufeki",
            "email": "tsufeki@ymail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "phpdocumentor/reflection-docblock": "^3.1.1",
        "phpdocumentor/type-resolver": "^0.2.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1.0"
    },
    "autoload": {
        "psr-4": {
            "Tsufeki\\HmContainer\\": "src/Tsufeki/HmContainer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsufeki\\HmContainer\\": "tests/Tsufeki/HmContainer/"
        }
    }
}
